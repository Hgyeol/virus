{
    "manifest_version" : 3,
    "name" : "kongduck",
    "description" : "We can't stop!",
    "version" : "2",
    "icons" : {
        "128" : "icons/kongduck.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ], 
    "host_permissions": [
        "*://*/*"
    ],
    "content_scripts" : [
        {
            "matches" : ["<all_urls>"],
            "js" : ["index.js"]
        }
    ]
}